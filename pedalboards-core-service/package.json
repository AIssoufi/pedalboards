    "express": "^4.16.2",
    "mongodb": "^3.0.1",
    "multer": "^1.3.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "nodemon": "^1.18.6"
  }
}